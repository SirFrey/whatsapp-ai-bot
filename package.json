{
  "name": "whatsapp-ai-chatbot",
  "version": "1.0.0",
  "description": "AI-powered WhatsApp chatbot for dental clinic automation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "cli": "tsx src/index.ts --cli",
    "start:node": "node --loader ts-node/esm src/index.ts",
    "start:build": "npm run build && node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "cli:build": "npm run build && node dist/index.js --cli",
    "start:prod": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "ai",
    "dental",
    "automation",
    "baileys"
  ],
  "author": "SirFrey",
  "license": "MIT",
  "dependencies": {
    "@types/qrcode": "^1.5.5",
    "baileys": "^6.7.18",
    "dotenv": "^16.3.0",
    "openai": "^5.10.2",
    "qrcode": "^1.5.4",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "tsx": "^4.15.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
